  "scripts": {
    "start": "node ./app.js",
    "dev": "nodemon ./app.js -p 8080 --mode=dev",
    "test": "node ./app.js -p 9000 --mode=test",
    "simplex": "artillery quick --count 20 --num 100 'http://localhost:8080/api/simplex' -o ./test/artillery/quick/simplex.json",
    "complex": "artillery quick --count 20 --num 100 'http://localhost:8080/api/complex' -o ./test/artillery/quick/complex.json",
    "flow": "artillery run ./test/artillery/flow.yaml -o ./test/artillery/flow.json",
    "report": "artillery report ./test/artillery/flow.json",
    "image": "docker build -t codercommerce .",
    "rename": "docker tag codercommerce martinpache/commerce:1.0.0",
    "push": "docker push martinpache/commerce:1.0.0",
    "docker": "docker run -p 9000:9000 codercommerce",
    "mocha": "mocha ./test/mocha --mode=test",
    "chai": "mocha ./test/chai --mode=test",
    "supertest": "mocha ./test/supertest --mode=test"
  },
